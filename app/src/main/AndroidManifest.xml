<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dinokeylas.toyotafunservice">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@drawable/icon_tfs"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_tfs"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.ServiceBookingActivity"/>
        <activity android:name=".view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".view.EmergencyCallActivity" />
        <activity android:name=".view.HomeActivity" />
    </application>

</manifest>